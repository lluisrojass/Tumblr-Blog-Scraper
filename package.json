{
  "name": "tumblr-scraper",
  "productName": "Tumblr Scraper",
  "version": "1.2.0",
  "main": "./app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lluisrojass/Tumblr-Scraper.git"
  },
  "description": "a utility to scrape a Tumblr blog for user uploaded posts",
  "scripts": {
    "bundle:prod": "webpack --config ./build-configs/webpack.prod.js",
    "bundle:dev": "webpack --config ./build-configs/webpack.dev.js",
    "dev": "webpack --watch --config ./build-configs/webpack.dev.js",
    "simulate": "cross-env NODE_ENV=development ELECTRON_ENABLE_STACK_DUMPING=1 electron ./app/index.js",
    "simulate:server": "cross-env SERVER_ONLY=1 DEBUG=* NODE_ENV=development ELECTRON_ENABLE_STACK_DUMPING=1 electron ./app/index.js",
    "flow:init": "flow init",
    "flow": "flow",
    "flow:stop": "flow stop",
    "build:osx": "cross-env NODE_ENV=production electron-packager ./ --icon='./Resources/icon.icns' --platform=darwin --arch=x64 --electron-version='1.4.15' --app-version='1.2.0'  --ignore='(dist|README.md)' --overwrite --osx-sign --out ./dist --app-bundle-id='com.builtbyluis.lluisrojass.TScraper' ",
    "build:linux": "cross-env NODE_ENV=production electron-packager . --icon=icon.png --platform=linux --electron-version='1.4.15' --app-version='1.2.0' --ignore='(dist|README.md)' --overwrite",
    "buid:win": "electron-packager . --icon=icon.png --platform=win32 --electron-version='1.4.15' --app-version='1.0.0' --ignore='(Tumblr Scraper-win32-.*|Tumblr Scraper-linux-.*|Tumblr Scraper-darwin-.*)' --overwrite"
  },
  "build": {
    "electronVersion": "1.4.15",
    "appId": "com.tumblrscraper",
    "productName": "Tumblr Scraper",
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "Resources/icon.icns",
      "type": "distribution"
    },
    "dmg": {
      "backgroundColor": "#89c657"
    },
    "win": {
      "target": "NSIS",
      "icon": "Resources/icon.ico"
    },
    "linux": {
      "icon": "Resources/icon.png",
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "author": "lluisrojass@aol.com",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "balloon-css": "^0.5.0",
    "classnames": "^2.2.6",
    "debounce": "^1.2.0",
    "electron-log": "^2.2.17",
    "get-port": "^4.1.0",
    "htmlparser2": "^3.9.2",
    "nmsp": "^1.1.0",
    "pipe-event": "^0.1.0",
    "react-redux": "^5.0.6",
    "react-svg-loader": "^2.1.0",
    "recompose": "^0.30.0",
    "socket.io": "^2.2.0",
    "unstated": "^2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "ajv": "^6.5.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.2",
    "babel-preset-flow": "^6.23.0",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "css-module-flow": "^1.0.0",
    "electron": "^4.0.1",
    "electron-builder": "^20.5.1",
    "electron-packager": "^8.7.1",
    "electron-reload": "^1.1.0",
    "eslint": "^5.7.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^3.0.1",
    "flow": "^0.2.3",
    "flow-bin": "^0.93.0",
    "flow-typed": "^2.5.1",
    "html-webpack-plugin": "^3.2.0",
    "postcss-functions": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.4.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "sinon": "^7.2.2",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-merge": "^4.1.4"
  },
  "bugs": {
    "url": "https://github.com/lluisrojass/Tumblr-Scraper/issues"
  },
  "homepage": "https://github.com/lluisrojass/Tumblr-Scraper#readme",
  "keywords": []
}
